function getLangClient() {
    return {
        isInitialized: true,
        getAST: (params) => {
            return new Promise((resolve, reject) => {
                setTimeout(() => resolve(JSON.parse(window.DiagramPanelBridge.getAst())), 0);
            })
        },
        getEndpoints: () => {
            return new Promise((resolve, reject) => {
                resolve({});
            })
        },
        astDidChange: (params) => {
            return new Promise((resolve, reject) => {
                resolve(window.DiagramPanelBridge.handleDiagramEdit(JSON.stringify(params)));
            })
        }
    }
}