    "webpack-dev-server": "^2.4.1",
    "yargs": "^1.3.3",
    "react-test-renderer": "^15.6.1",
    "enzyme": "^2.9.1",
    "uuid": "^3.1.0",
    "babel-plugin-module-resolver": "^2.7.0",
    "ws": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "circular-dependency-plugin": "^3.0.0",
