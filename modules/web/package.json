{
  "name": "ballerina-composer",
  "version": "0.92.1",
  "description": "A web based tool for creating integrations in ballerina language.",
  "scripts": {
    "clean": "rimraf dist",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "build-electron": "cross-env NODE_ENV=electron webpack --config webpack.config.js",
    "dev": "webpack-dev-server --hot --inline --progress",
    "dev-electron": "cross-env NODE_ENV=electron-dev webpack-dev-server --hot --inline --progress",
    "test-spec": "cross-env NODE_ENV=test mocha-webpack --require ./js/tests/js/spec/setup.js --webpack-config ./webpack.config.js ./js/tests/js/spec/ballerina-test.js;",
    "test": "cross-env NODE_ENV=test mocha-webpack --require ./js/tests/js/spec/setup.js --webpack-config ./webpack.config.js 'js/tests/js/spec/*test.js';",
    "test-ui": "cross-env NODE_ENV=test mocha-webpack --require ./js/tests/js/spec/setup.js --webpack-config ./webpack.config.js ./react-test/__tests__/component-tests.js;",
    "test-debug": "cross-env NODE_ENV=test node --inspect ./node_modules/mocha-webpack/lib/cli --require ./js/tests/js/spec/setup.js --webpack-config ./webpack.config.js 'js/tests/js/spec/*test.js' --timeout 600000;",
    "lint": "eslint . --ext .jsx --ext .js",
    "lintfile": "eslint",
    "storybook": "start-storybook -p 6006 --dont-track",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ballerinalang/composer.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ballerinalang/composer/issues"
  },
  "homepage": "https://github.com/ballerinalang/composer#readme",
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "babel-cli": "^6.24.0",
    "babel-preset-es2015": "^6.22.0",
    "chai": "^3.5.0",
    "cross-env": "^3.2.3",
    "css-loader": "^0.26.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "jsdom": "^9.11.0",
    "mocha": "^3.2.0",
    "mocha-webpack": "^0.7.0",
    "node-sass": "^4.5.2",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "shelljs": "^0.7.6",
