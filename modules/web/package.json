    "html5shiv": "^3.7.3",
    "jquery": "^1.9.1",
    "jquery-contextmenu": "^2.4.2",
    "jquery-mousewheel": "^3.1.13",
    "jquery-ui": "^1.12.1",
    "js-yaml": "^3.7.0",
    "jsplumb": "2.3.5",
