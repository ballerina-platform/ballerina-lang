{
  "name": "ballerina-composer",
  "version": "0.91.1",
  "description": "A web based tool for creating integrations in ballerina language.",
  "scripts": {
    "clean": "rimraf dist",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "build-electron": "cross-env NODE_ENV=electron webpack --config webpack.config.js",
    "dev": "webpack-dev-server --hot --inline --progress",
    "dev-electron": "cross-env NODE_ENV=electron-dev webpack-dev-server --hot --inline --progress",
    "test-spec": "cross-env NODE_ENV=test mocha-webpack --require ./js/tests/js/spec/setup.js --webpack-config ./webpack.config.js ./js/tests/js/spec/BallerinaTest.js;",
    "test": "node run-tests.js",
    "lint": "eslint . --ext .jsx --ext .js",
    "lintfile": "eslint",
    "storybook": "start-storybook -p 6006 --dont-track",
    "build-storybook": "build-storybook",
    "test-jest": "jest ./react-test/"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "unmockedModulePathPatterns": [
      "react"
    ],
    "moduleNameMapper": {
      "svg_pan_zoom": "svg-panNZoom/jquery.svg.pan.zoom",
      "theme_wso2": "theme-wso2-2.0.0/js/theme-wso2",
      "mcustom_scroller": "malihu-custom-scrollbar-plugin",
      "respond": "respond_1.4.2/respond.min",
      "select2": "select2-4.0.3/dist/js/select2.full.min",
      "underscore": "lodash",
      "ace": "ace-builds/src-noconflict",
      "dagre": "dagre-0.7.4/dagre.min.js",
      "log": "log/log",
      "d3utils": "utils/d3-utils",
      "diagram_core": "diagram-core/module",
      "command": "command/command",
      "event_channel": "event/channel",
      "drag_drop_manager": "drag-drop/manager",
      "main_elements": "main-elements/module",
      "processors": "processors/module",
      "file_browser": "file-browser/file-browser",
      "menu_bar": "menu-bar/menu-bar",
      "context_menu": "context-menu/context-menu",
      "tool_bar": "tool-bar/tool-bar",
      "alerts": "utils/alerts",
      "breadcrumbs": "breadcrumbs/breadcrumbs",
      "property_pane_utils": "property-pane/property-pane-utils",
      "expression_editor_utils": "expression-editor/expression-editor-utils",
      "constants": "constants/constants",
      "typeMapper": "type-mapper/type-mapper-renderer",
      "environment_content": "ballerina/env/environment-content",
      "bal_utils": "ballerina/utils",
      "bal_configs": "ballerina/configs",
      "console": "launcher/console",
      "workspace$": "workspace/module",
      "ballerina$": "ballerina/module",
      "welcome-page$": "welcome-page/module",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "moduleDirectories": ["lib", "js", "node_modules","react-test"],
    "modulePaths": ["lib", "js", "node_modules"]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ballerinalang/composer.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ballerinalang/composer/issues"
  },
  "homepage": "https://github.com/ballerinalang/composer#readme",
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "babel-cli": "^6.24.0",
    "babel-preset-es2015": "^6.22.0",
    "chai": "^3.5.0",
    "cross-env": "^3.2.3",
    "css-loader": "^0.26.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "jsdom": "^9.11.0",
    "mocha": "^3.2.0",
    "mocha-webpack": "^0.7.0",
    "node-sass": "^4.5.2",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "shelljs": "^0.7.6",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "yargs": "^1.3.3",
    "jest": "^20.0.4",
    "babel-jest": "^20.0.3",
    "react-test-renderer":"^15.6.1",
    "enzyme": "^2.9.1",
    "jest-enzyme":"^3.6.1",
    "babel-plugin-module-resolver": "^2.7.0",
    "ws":"^3.1.0"
  },
  "dependencies": {
    "ace-builds": "^1.2.6",
    "antlr4": "^4.6.0",
    "axios": "^0.16.2",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-react": "^6.23.0",
    "backbone": "^1.3.3",
    "bootstrap": "^3.3.6",
    "brace": "^0.10.0",
    "classnames": "^2.2.5",
    "d3": "^4.1.1",
    "golden-layout": "github:deepstreamio/golden-layout",
    "html-loader": "^0.4.5",
    "html5shiv": "^3.7.3",
    "jquery": "^1.9.1",
    "jquery-contextmenu": "^2.4.2",
    "jquery-mousewheel": "^3.1.13",
    "jquery-ui": "^1.12.1",
    "js-yaml": "^3.7.0",
    "jsplumb": "2.3.5",
    "jstree": "^3.3.4",
    "lodash": "^4.13.1",
    "log4javascript": "^1.4.15",
    "malihu-custom-scrollbar-plugin": "^3.1.5",
    "mousetrap": "^1.6.0",
    "npm": "^4.2.0",
    "prop-types": "^15.5.8",
    "raw-loader": "^0.5.1",
    "react": "^15.4.2",
    "react-autosuggest": "^9.0.1",
    "react-bootstrap": "^0.31.1",
    "react-collapsible": "^1.4.0",
    "react-custom-scrollbars": "^4.1.2",
    "react-dom": "^15.4.2",
    "react-transition-group": "^1.1.2",
    "select2": "^4.0.3",
    "typeahead.js": "^0.11.1",
    "unused-files-webpack-plugin": "^3.0.1"
  }
}
