<!DOCTYPE html>
<html>

{{> head }}

<body>
    <div class="navi-wrapper">
        <div class="navi-wrapper-content">
            {{> logo }}
            
            {{> currentModule }}
            {{> allModules }}
        </div>
    </div>

    <div class="ui vertical sidebar menu">
        <div class="navi-wrapper-sidebar">
            <div class="navi-wrapper-content">
                {{> logo }}
                
                {{> currentModule }}
                {{> allModules }}
            </div>
        </div>
    </div>

    <div class="content-wrapper pusher">
        <div>
            <a class="toc item"> <img src="../html-template-resources/images/menu.svg"></a>
        </div>
        <div class="main-wrapper">
            <h1>Constants</h1>
            <div class="constants">
                <div class="data-wrapper">
                    {{#each constants}}
                        <div class="params-listing">
                            <ul>
                                <li id="{{ name }}"> <b>{{ name }}</b><span class="type">  {{{ typeName type }}}</span> {{ value }}</li>
                                <li>
                                    {{{ description }}}
                                </li>
                            </ul>
                        </div>
                    {{/each}}
                </div>
            </div>
        </div>
    </div>
</body>

{{> footer }}
