<!DOCTYPE html>
<html>

{{> head }}

<body class="cBallerina-io">
    {{> bioBody }}
    <div class="navi-wrapper">
        <div class="navi-wrapper-content">
            {{> logo }}
            {{> currentModule }}
            {{> allModules }}
        </div>
    </div>

    <div class="ui vertical sidebar menu">
        <div class="navi-wrapper-sidebar">
            <div class="navi-wrapper-content">
                {{> logo }}
                
                {{> currentModule }}
                {{> allModules }}
            </div>
        </div>
    </div>



    <div class="content-wrapper pusher">
        {{> mobMenu }}
        <div class="main-wrapper module-wrapper">
            <div id="search-impl"></div>
            <div id="main">
                <h1>
                    {{> breadcrumb }}
                    {{#if project.isSingleFile}}
                        {{project.sourceFileName}}
                    {{else}}
                        Module : <span class="modules">{{ module.id }}</span>
                    {{/if}}
                </h1>
                    {{{ module.description }}}

            {{#if module.records}}

            <section class="method-content" id="records">
                <div class="main-method-title here">
                    <a class="url-link" href="#records">
                        <img class="url-link-img" src="{{ rootPath }}html-template-resources/images/link.svg">
                    </a>
                <h2> Records </h2>
                </div>
                <table>
                    {{#each module.records}}
                        <tr>
                            <td class="module-title truncate records" id="{{name}}" title="{{name}}">
                                <a class="records {{#if isDeprecated}}strike{{/if}}" href="records/{{name}}.html">{{name}}</a>
                            </td>
                            <td class="module-desc">
                                {{#if isDeprecated}}
                                    <span class="stab">Deprecated</span>
                                {{/if}}
                                {{#if isAnonymous}}Anonymous record{{else}}{{{ editDescription description }}}{{/if}}
                            </td>
                        </tr>
                    {{/each}}
                </table>
            </section>
            {{/if}}

            {{#if module.objects}}
            <section class="method-content" id="objects">
                <div class="main-method-title here">
                    <a class="url-link" href="#objects">
