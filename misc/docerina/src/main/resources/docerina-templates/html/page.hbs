<!DOCTYPE html>
<html class="no-js" lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="shortcut icon" href="docerina-theme/img/favicon.ico">
    <title>Ballerina - {{heading.value}} API Docs</title>
    <script src="docerina-theme/js/jquery-3.3.1.min.js"></script>
    <link rel="stylesheet" href="docerina-theme/css/bootstrap.min.css">
    <!-- Optional theme -->
    <link rel="stylesheet" href="docerina-theme/css/bootstrap-theme.min.css">
    <!-- Latest compiled and minified JavaScript -->
    <script src="docerina-theme/js/bootstrap.min.js"></script>
    <!-- Nanoscroller JS -->
    <script src="docerina-theme/js/jquery.nanoscroller.min.js"></script>
    <!-- Docerina JS -->
    <script src="docerina-theme/js/docerina.js"></script>
    <script type="text/javascript" src="docerina-theme/js/highlight.pack.js"></script>
    <link rel="stylesheet" href="docerina-theme/css/theme.css" type="text/css" />
    <link rel="stylesheet" href="docerina-theme/css/theme_extra.css" type="text/css" />
    <link rel="stylesheet" href="docerina-theme/css/highlight.css">
    <script type="text/javascript" src="docerina-theme/js/ballerina-common.js"></script>
    <link rel="stylesheet" href="docerina-theme/css/ballerina-io.css">
    <link rel="stylesheet" href="docerina-theme/css/ballerina-io-docs.css">
    <link rel="stylesheet" href="docerina-theme/css/ballerina-io-api.css">
    <script type="text/javascript">
        $(document).ready(function() {
            if (typeof hljs === 'object') {
                $('code.language-ballerina').addClass('ballerina');
                hljs.initHighlighting();
            }
        });
    </script>
    <script>
        var base_url = "";
    </script>
    <script src="/search/require.js"></script>
</head>

<body class="cBallerina-io" role="document">
<!-- Google Tag Manager (noscript) -->
<noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-PSL2TX4"
                  height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
<!-- End Google Tag Manager (noscript) -->
<div class="row cBallerina-io-top-row">
    <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6"></div>
    <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6 cTopLineContainer">
        <div class="cTopLine"></div>
    </div>
</div>
<div class="row cBallerina-io-Logo-row">
    <div class="container">
        <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6 cBallerina-io-Logo-Container">
            <a class="cLogo" href="index.html">
                <img src="docerina-theme/img/ballerina-logo.png" alt="Ballerina" />
            </a>
        </div>
    </div>
</div>
<div class="row cBallerina-io-Gray-row">
    <div class="container">
        <div class="col-xs-12 col-sm-12 col-md-3 col-lg-3 cBallerina-io-Home-Left-col">
            <div class="cBlallerina-io-docs-right-nav-container">
                <div class="cLectColStyle cLeftColTop">
                    <div class="cBallerina-io-docs-Title cBallerina-io-primitive-types">PRIMITIVE TYPES</div>
                    <div class="cCollaps-Menu-first">
                        {{#each primitives}}
                            <ul class="">
                                <li>
                                    <a href="{{href}}">
                                        <span class="bold">{{caption.value}}</span>
                                    </a>
                                </li>
                            </ul>
                        {{/each}}
                    </div>
                </div>
            </div>
            <div class="cBlallerina-io-docs-right-nav-container">
                <div class="cLectColStyle cLeftColTop">
                    <div class="cBallerina-io-docs-Title cBallerina-io-packages">BALLERINA MODULES</div>

                    <div class="cCollaps-Menu" id="iBallerinaPACKAGEStoc">

                        {{#each links}}
                        <ul>
                                <li class="cTableTitle">
                                    <a href="{{href}}.html">
                                        {{#if caption.isPackageName}}
                                            {{caption.prefix}}<span class="bold">{{caption.suffix}}</span>
                                        {{/if}}
                                        {{#if caption.isStaticCaption}}
                                            <span class="bold">{{caption.value}}</span>
                                        {{/if}}
                                    </a>
                                </li>
                        </ul>
                        {{/each}}

                    </div>
                </div>
            </div>
            <div class="cBlallerina-io-docs-left-nav-container">
                <div class="cSideBar-Button">
                    <img src="docerina-theme/img/sidebar.svg" />
                </div>
                <div class="cLectColStyle">
                    <div class="cBallerina-io-docs-Title">TABLE OF CONTENTS</div>
                    {{#each links}}
                        <ul class=" {{#if active}}active-package{{/if}}">
                            <li>
                                <!--Package Name-->
                                {{#if active}} {{#if caption.isPackageName}}
                                    <a class="cPackageList">{{caption.prefix}}<span class="bold">{{caption.suffix}}</span></a> {{/if}} {{#if caption.isStaticCaption}}
                                    <a>
                                        <span class="bold">{{caption.value}}</span>
                                    </a>
                                {{/if}}
                                    <div class="constructs-wrapper">
                                        <!--Construct of Package-->
                                        <ul class="constructs-ul">
                                            {{#exists constructs "primitive"}}
                                                <li>
                                                    <a href="#PrimitivesSummary">
                                                        <div class="construct-icon">
                                                            <span class="fw-primitive"> </span>
                                                        </div>
                                                        <div class="construct-name">
                                                            Primitives
                                                        </div>
                                                    </a>
                                                </li>
                                            {{/exists}}
                                            {{#exists constructs "record"}}
                                                <li>
                                                    <a href="#RecordsSummary">
                                                        <div class="construct-icon">
                                                            <span class="fw-record"> </span>
                                                        </div>
                                                        <div class="construct-name">
                                                            Records
                                                        </div>
                                                    </a>
                                                </li>
                                            {{/exists}}
                                            {{#exists constructs "object"}}
                                                <li>
                                                    <a href="#ObjectsSummary">
                                                        <div class="construct-icon">
                                                            <span class="fw-struct"> </span>
                                                        </div>
                                                        <div class="construct-name">
                                                            Objects
                                                        </div>
                                                    </a>
                                                </li>
                                            {{/exists}}
                                            {{#exists constructs "endpoint"}}
                                                <li>
                                                    <a href="#EndpointsSummary">
                                                        <div class="construct-icon">
                                                            <span class="fw-endpoint"> </span>
                                                        </div>
                                                        <div class="construct-name">
                                                            Endpoints
                                                        </div>
                                                    </a>
                                                </li>
                                            {{/exists}}
                                            {{#exists constructs "function"}}
                                                <li>
                                                    <a href="#FunctionsSummary">
                                                        <div class="construct-icon">
                                                            <span class="fw-function"> </span>
                                                        </div>
                                                        <div class="construct-name">
                                                            Functions
                                                        </div>
                                                    </a>
                                                </li>
                                            {{/exists}}
                                            {{#exists constructs "type"}}
                                                <li>
                                                    <a href="#TypeDefinitions">
                                                        <div class="construct-icon">
                                                            <span class="fw-type"> </span>
                                                        </div>
                                                        <div class="construct-name">
                                                            Type Definitions
                                                        </div>
                                                    </a>
                                                </li>
                                            {{/exists}}
                                            {{#exists constructs "globalvar"}}
                                                <li>
                                                    <a href="#GlobalVariables">
                                                        <div class="construct-icon">
                                                            <span class="fw-globe"> </span>
                                                        </div>
                                                        <div class="construct-name">
                                                            Global Variables
                                                        </div>
                                                    </a>
                                            </li>
                                            {{/exists}}
                                            {{#exists constructs "annotation"}}
                                                <li>
                                                    <a href="#Annotations">
                                                        <div class="construct-icon">
                                                            <span class="fw-annotation"> </span>
                                                        </div>
                                                        <div class="construct-name">
                                                            Annotations
                                                        </div>
                                                    </a>
                                                </li>
                                            {{/exists}}

                                            {{#each constructs}}
                                                {{#unless isGlobalVariable}} {{#unless isType}} {{#unless isAnnotation}}
                                                    <li>
                                                        <a href="#{{name}}">
                                                            <div class="construct-icon">
                                                                <span class="{{icon}}"> </span>
                                                            </div>
                                                            <div class="construct-name">
                                                                {{name}}
                                                            </div>
                                                        </a>
                                                    </li>
                                                    {{#each children}}
                                                        <ul class="construct-inner-items-ul">
                                                            <li>
                                                                <a href="#{{../name}}-{{name}}">
                                                                    <div class="construct-icon">
                                                                        <span class="{{icon}}"> </span>
                                                                    </div>
                                                                    <div class="construct-name">
                                                                        {{name}}
                                                                    </div>
                                                                </a>
                                                            </li>
                                                        </ul>
                                                    {{/each}} {{/unless}} {{/unless}} {{/unless}} {{/each}}
                                        </ul>
                                    </div>
                                {{else}} {{!--
                                <a href="{{href}}.html">
                           {{#if caption.isPackageName}} {{caption.prefix}}
                           <span class="bold">{{caption.suffix}}</span>
                           {{/if}} {{#if caption.isStaticCaption}}
                           <span class="bold">{{caption.value}}</span>
                           {{/if}}
                           </a> --}} {{/if}}
                            </li>
                        </ul>
                    {{/each}}
                </div>
            </div>
        </div>
        <div class="col-xs-12 col-sm-12 col-md-6 col-lg-9 cBallerina-io-Home-Middle-col">
            <div class="cBlallerina-io-docs-content-container">
                <div>
                    <div>
                        <div role="main">
                            <div>
                                <div id="content" class="nano">
                                    <div class="nano-content">
                                        <h1 class="thin-text padding-bottom">
                                            <span class="bold">{{heading.value}}</span> {{#if heading.isPackageName}} module {{/if}}
                                        </h1>
                                        {{#if description}}
                                            <div class="cAPIdocsDescription">
                                                {{{description}}}
                                            </div>
                                        {{/if}}

                                        <div class="summary-wrapper">
                                            <!--Primitive Types-->
                                            {{#exists constructs "primitive"}}
                                                <h2 id="PrimitivesSummary">
                                                    <span class="fw-primitive sm-icon"></span> Primitives Summary
                                                </h2>
                                                <table class="cPrimitive-summary-Table">
                                                    <tr>
                                                        <th class="cSummary-table-type">Type</th>
                                                        <th class="cSummary-table-desc">Description</th>
                                                    </tr>
                                                    {{#each constructs}}
                                                        {{#if isPrimitiveType}}
                                                            <tr>
                                                                <td class="cSummary-table-type"><a href="#{{name}}">{{name}}</a></td>
                                                                <td class="cSummary-table-desc">{{{description}}}</td>
                                                            </tr>
                                                        {{/if}}
                                                    {{/each}}
                                                </table>
                                            {{/exists}}
                                            <!-- Type Definitions-->
                                            {{#exists constructs "type"}}
                                                <h2 id="TypeDefinitions">
                                                    <span class="fw-type sm-icon"></span> Type Definitions
                                                </h2>
                                                <table class="cSummary-Table">
                                                    <tr>
                                                        <th class="cSummary-table-name">Type</th>
                                                        <th class="cSummary-table-attach"></th>
                                                        <th class="cSummary-table-type">Values</th>
                                                        <th class="cSummary-table-desc">Description</th>
                                                    </tr>
                                                    {{#each constructs}}
                                                        {{#if isType}}
                                                            <tr>
                                                                <td class="cSummary-table-name">{{name}}</td>
                                                                <td class="cSummary-table-attach"></td>
                                                                <td class="cSummary-table-type">{{valueSet}}</td>
                                                                <td class="cSummary-table-desc">{{{description}}}</td>
                                                            </tr>
                                                        {{/if}}  {{/each}}
                                                </table>
                                            {{/exists}}
                                            <!-- Annotations -->
                                            {{#exists constructs "annotation"}}
                                                <h2 id="Annotations">
                                                    <span class="fw-annotation sm-icon"></span> Annotations
                                                </h2>
                                                <table class="cSummary-Table">
                                                    <tr>
                                                        <th class="cSummary-table-name">Name</th>
                                                        <th class="cSummary-table-attach">Attachement Points</th>
                                                        <th class="cSummary-table-type">Data Type</th>
                                                        <th class="cSummary-table-desc">Description</th>
                                                    </tr>
                                                    {{#each constructs}}
                                                        {{#if isAnnotation}}
                                                            <tr>
                                                                <td class="cSummary-table-name">{{name}}</td>
                                                                <td class="cSummary-table-attach">{{attachments}}</td>
                                                                <td class="cSummary-table-type"><a href="{{href}}">{{dataType}}</a></td>
                                                                <td class="cSummary-table-desc">{{{description}}}</td>
                                                            </tr>
                                                        {{/if}}  {{/each}}
                                                </table>
                                            {{/exists}}
                                            <!-- records -->
                                            {{#exists constructs "record"}}
                                                <h2 id="RecordsSummary">
                                                    <span class="fw-record sm-icon"></span> Records Summary
                                                </h2>
                                                <table class="cSummary-Table">
                                                    <tr>
                                                        <th class="cSummary-table-name">Record</th>
                                                        <th class="cSummary-table-attach"></th>
                                                        <th class="cSummary-table-type"></th>
                                                        <th class="cSummary-table-desc">Description</th>
                                                    </tr>
                                                    {{#each constructs}}
                                                        {{#if isRecord}}
                                                            <tr>
                                                                <td class="cSummary-table-name"><a href="#{{name}}">{{name}}</a></td>
                                                                <td class="cSummary-table-attach"></td>
                                                                <td class="cSummary-table-type"></td>
                                                                <td class="cSummary-table-desc">{{{description}}}</td>
                                                            </tr>
                                                        {{/if}}
                                                    {{/each}}
                                                </table>
                                            {{/exists}}
                                            <!-- objects -->
                                            {{#exists constructs "object"}}
                                                <h2 id="ObjectsSummary">
                                                    <span class="fw-struct sm-icon"></span> Objects Summary
                                                </h2>
                                                <table class="cSummary-Table">
                                                    <tr>
                                                        <th class="cSummary-table-name">Object</th>
                                                        <th class="cSummary-table-attach"></th>
                                                        <th class="cSummary-table-type"></th>
                                                        <th class="cSummary-table-desc">Description</th>
                                                    </tr>
                                                    {{#each constructs}}
                                                        {{#if isObject}}
                                                            <tr>
                                                                <td class="cSummary-table-name"><a href="#{{name}}">{{name}}</a></td>
                                                                <td class="cSummary-table-attach"></td>
                                                                <td class="cSummary-table-type"></td>
                                                                <td class="cSummary-table-desc">{{{description}}}</td>
                                                            </tr>
                                                        {{/if}}
                                                    {{/each}}
                                                </table>
                                            {{/exists}}
                                            <!-- endpoints -->
                                            {{#exists constructs "endpoint"}}
                                                <h2 id="EndpointsSummary">
                                                    <span class="fw-endpoint sm-icon"></span> Endpoints Summary
                                                </h2>
                                                <table class="cSummary-Table">
                                                    <tr>
                                                        <th class="cSummary-table-name">Endpoint</th>
                                                        <th class="cSummary-table-attach"></th>
                                                        <th class="cSummary-table-type"></th>
                                                        <th class="cSummary-table-desc">Description</th>
                                                    </tr>
                                                    {{#each constructs}}
                                                        {{#if isConnector}}
                                                            <tr>
                                                                <td class="cSummary-table-name"><a href="#{{name}}">{{name}}</a></td>
                                                                <td class="cSummary-table-attach"></td>
                                                                <td class="cSummary-table-type"></td>
                                                                <td class="cSummary-table-desc">{{{description}}}</td>
                                                            </tr>
                                                        {{/if}}
                                                    {{/each}}
                                                </table>
                                            {{/exists}}
                                            <!-- functions -->
                                            {{#exists constructs "function"}}
                                                <h2 id="FunctionsSummary">
                                                    <span class="fw-function sm-icon"></span> Functions Summary
                                                </h2>
                                                <table class="cSummary-Table">
                                                    <tr>
                                                        <th class="cSummary-table-name">Return Type</th>
                                                        <th class="cSummary-table-attach"></th>
                                                        <th class="cSummary-table-type"></th>
                                                        <th class="cSummary-table-desc">Function and Description</th>
                                                    </tr>
                                                    {{#each constructs}}
                                                        {{#if isFunction}}
                                                            <tr>
                                                                <td class="cSummary-table-name">{{#if returnParams}}{{join returnParams ", "}}{{/if}}</td>
                                                                <td class="cSummary-table-attach"></td>
                                                                <td class="cSummary-table-type"></td>
                                                                <td class="cSummary-table-desc"><a href="#{{name}}">{{name}}({{join parameters ", "}})</a><br/>{{{description}}}</td>
                                                            </tr>
                                                        {{/if}}
                                                    {{/each}}
                                                </table>
                                            {{/exists}}
                                            <!-- Global Variables-->
                                            {{#exists constructs "globalvar"}}
                                                <h2 id="GlobalVariables">
                                                    <span class="fw-globe sm-icon"></span> Global Variables
                                                </h2>
                                                <table class="cSummary-Table">
                                                    <tr>
                                                        <th class="cSummary-table-name">Name</th>
                                                        <th class="cSummary-table-attach"></th>
                                                        <th class="cSummary-table-type">Data Type</th>
                                                        <th class="cSummary-table-desc">Description</th>
                                                    </tr>
                                                    {{#each constructs}}
                                                        {{#if isGlobalVariable}}
                                                            <tr>
                                                                <td class="cSummary-table-name">{{name}}</td>
                                                                <td class="cSummary-table-attach"></td>
                                                                <td class="cSummary-table-type"><a href="{{href}}">{{dataType}}</a></td>
                                                                <td class="cSummary-table-desc">{{{description}}}</td>
                                                            </tr>
                                                        {{/if}}  {{/each}}
                                                </table>
                                            {{/exists}}
                                        </div>

                                        {{#each constructs}}
                                            <div class="construct-wrapper">
                                                <!--Primitive Types-->
                                                {{#if isPrimitiveType}}
                                                    <h2 id="{{name}}" class="cPrimitive">
                                                        <span class="{{icon}} sm-icon"></span> {{name}}
                                                    </h2>
                                                    {{#if description}}
                                                        <div class="cPrimitiveDescription">
                                                            <span class="fw-right gray xs-icon"></span>{{{description}}}
                                                        </div>
                                                    {{/if}} {{#if children}}
                                                    <div class="function-wrapper">
                                                        <ul>
                                                            {{#each children}}
                                                                <li class="cFunctionList">
                                                                    <h2 id="{{../name}}-{{name}}">
                                                                        <span class="{{icon}}"></span>
                                                                        <span class="thin-text light-gray">&lt;{{../name}}&gt;</span> {{name}}({{join parameters ", "}}) {{#if returnParams}}returns ({{join returnParams ", "}}) {{/if}}
                                                                    </h2>
                                                                    {{#if description}}
                                                                        <p>
                                                                            <span class="fw-right gray xs-icon"></span>{{{description}}}
                                                                        </p>
                                                                    {{/if}} {{#if parameters}}
                                                                    <table>
                                                                        <tr>
                                                                            <th class="cAPI-table-name">Parameter Name</th>
                                                                            <th class="cAPI-table-type">Data Type</th>
                                                                            <th class="cAPI-table-default">Default Value</th>
                                                                            <th class="cAPI-table-desc">Description</th>
                                                                        </tr>
                                                                        {{#each parameters}}
                                                                            <tr>
                                                                                <td class="cAPI-table-name">{{name}}</td>
                                                                                <td class="cAPI-table-type">{{{dataTypeLink dataType href}}}</td>
                                                                                <td class="cAPI-table-default">{{defaultValue}}</td>
                                                                                <td class="cAPI-table-desc">{{{description}}}</td>
                                                                            </tr>
                                                                        {{/each}}
                                                                    </table>
                                                                {{/if}} {{#if returnParams}}
                                                                    <div class="cReturnTableHeader"></div>
                                                                    <table>
                                                                        <tr>
                                                                            <th class="cAPI-table-name"></th>
                                                                            <th class="cAPI-table-type">Return Type</th>
                                                                            <th class="cAPI-table-default"></th>
                                                                            <th class="cAPI-table-desc">Description</th>
                                                                        </tr>
                                                                        {{#each returnParams}}
                                                                            <tr>
                                                                                <td class="cAPI-table-name"></td>
                                                                                <td class="cAPI-table-type">{{{dataTypeLink dataType href}}}</td>
                                                                                <td class="cAPI-table-default"></td>
                                                                                <td class="cAPI-table-desc">{{{description}}}</td>
                                                                            </tr>
                                                                        {{/each}}
                                                                    </table>
                                                                {{/if}}
                                                                </li>
                                                            {{/each}}
                                                        </ul>
                                                    </div>
                                                {{/if}} {{/if}}
                                                <!-- Function -->
                                                {{#if isFunction}}
                                                    <h2 id="{{name}}">
                                                        <span class="{{icon}} sm-icon"> </span> public function {{name}}({{join parameters ", "}}) {{#if returnParams}}returns ({{join returnParams ", "}}) {{/if}}
                                                    </h2>
                                                    {{#if description}}
                                                        <p>
                                                            <span class="fw-right gray xs-icon"></span>{{{description}}}
                                                        </p>
                                                    {{/if}} {{#if parameters}}
                                                    <table>
                                                        <tr>
                                                            <th class="cAPI-table-name">Parameter Name</th>
                                                            <th class="cAPI-table-type">Data Type</th>
                                                            <th class="cAPI-table-default">Default Value</th>
                                                            <th class="cAPI-table-desc">Description</th>
                                                        </tr>
                                                        {{#each parameters}}
                                                            <tr>
                                                                <td class="cAPI-table-name">{{name}}</td>
                                                                <td class="cAPI-table-type">{{{dataTypeLink dataType href}}}</td>
                                                                <td class="cAPI-table-default">{{defaultValue}}</td>
                                                                <td class="cAPI-table-desc">{{{description}}}</td>
                                                            </tr>
                                                        {{/each}}
                                                    </table>
                                                {{/if}} {{#if returnParams}}
                                                    <div class="cReturnTableHeader"></div>
                                                    <table>
                                                        <tr>
                                                            <th class="cAPI-table-name"></th>
                                                            <th class="cAPI-table-type">Return Type</th>
                                                            <th class="cAPI-table-default"></th>
                                                            <th class="cAPI-table-desc">Description</th>
                                                        </tr>
                                                        {{#each returnParams}}
                                                            <tr>
                                                                <td class="cAPI-table-name"></td>
                                                                <td class="cAPI-table-type">{{{dataTypeLink dataType href}}}</td>
                                                                <td class="cAPI-table-default"></td>
                                                                <td class="cAPI-table-desc">{{{description}}}</td>
                                                            </tr>
                                                        {{/each}}
                                                    </table>
                                                {{/if}} {{/if}}
                                                <!-- Struct-binded functions grouped together -->
                                                {{#if isStruct}}
                                                    <h2 id="{{name}}">
                                                        <span class="{{icon}} sm-icon"></span> public object {{name}}
                                                    </h2>
                                                    {{#if description}}
                                                        <p>
                                                            <span class="fw-right gray xs-icon"></span>{{{description}}}
                                                        </p>
                                                    {{/if}} {{#if fields}}
                                                    <table>
                                                        <tr>
                                                            <th class="cAPI-table-name">Field Name</th>
                                                            <th class="cAPI-table-type">Data Type</th>
                                                            <th class="cAPI-table-default">Default Value</th>
                                                            <th class="cAPI-table-desc">Description</th>

                                                        </tr>
                                                        {{#each fields}}
                                                            <tr>
                                                                <td class="cAPI-table-name">{{name}}</td>
                                                                <td class="cAPI-table-type">{{{dataTypeLink dataType href}}}</td>
                                                                <td class="cAPI-table-default">{{defaultValue}}</td>
                                                                <td class="cAPI-table-desc">{{{description}}}</td>

                                                            </tr>
                                                        {{/each}}
                                                    </table>
                                                {{/if}} {{#if children}}
                                                    <div class="function-wrapper">
                                                        <ul>
                                                            {{#each children}}
                                                                <li>
                                                                    <h2 id="{{../name}}-{{name}}">
                                                                        <span class="{{icon}}"></span>
                                                                        <span class="thin-text light-gray">&lt;{{../name}}&gt;</span> {{name}}({{join parameters ", "}}) {{#if returnParams}}returns ({{join returnParams ", "}}) {{/if}}
                                                                    </h2>
