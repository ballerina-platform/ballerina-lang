<FindBugsFilter>
    <Match>
        <Or>
            <And>
                <Class name="io.ballerina.semver.checker.diff.NodeListDiffImpl"/>
                <Bug pattern="UPM_UNCALLED_PRIVATE_METHOD"/>
            </And>
            <And>
                <Class name="io.ballerina.semver.checker.comparator.FunctionComparator"/>
                <Bug pattern="DLS_DEAD_LOCAL_STORE"/>
            </And>
        </Or>
    </Match>
    <Match>
        <Package name="io.ballerina.semver.checker.diff"/>
        <Bug pattern="EI_EXPOSE_REP,EI_EXPOSE_REP2"/>
    </Match>
    <Match>
        <Package name="io.ballerina.semver.checker"/>
        <Bug pattern="EI_EXPOSE_REP2"/>
    </Match>
</FindBugsFilter>
