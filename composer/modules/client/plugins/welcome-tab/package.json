{
  "name": "@ballerina-lang/composer-welcome-tab-plugin",
  "version": "0.962.0",
  "description": "Welome Tab plugin for composer",
  "homepage": "https://github.com/ballerina-lang/ballerina/composer/modules/client/plugins/welcome-tab",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "bugs": {
    "url": "https://github.com/ballerina-lang/ballerina/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ballerina-lang/ballerina.git"
  },
  "scripts": {
    "clean": "rimraf dist",
    "copy-assets": "copyfiles -u 1 'src/**/*.css' 'src/**/*.scss' 'src/**/*.json' dist",
    "build": "npm run clean && npm run copy-assets && babel src -d dist"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "copyfiles": "^1.2.0",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "@ballerina-lang/composer-core": "0.962.0",
    "@ballerina-lang/composer-api-client": "0.962.0",
    "@ballerina-lang/composer-help-plugin": "0.962.0",
    "prop-types": "^15.5.8",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "lodash": "^4.13.1"
  }
}
