    "style-loader": "^0.13.2",
    "uglifyjs-webpack-plugin": "^1.1.2",
    "unused-files-webpack-plugin": "^3.0.2",
    "url-loader": "^0.5.8",
    "webpack-dev-server": "^2.9.5",
    "webpack-webfont": "^0.0.1-alpha.8",
    "write-file-webpack-plugin": "^4.2.0",
    "ws": "^3.3.2",
    "yargs": "^1.3.3"
  },
  "dependencies": {
    "@pahans/react-htmltree": "^0.8.1",
    "ace-builds": "^1.2.6",
    "axios": "^0.16.2",
    "brace": "^0.10.0",
    "classnames": "^2.2.5",
    "copy-to-clipboard": "^3.0.8",
    "copy-webpack-plugin": "^4.2.3",
    "cross-env": "^3.2.3",
    "enzyme-adapter-react-16": "^1.1.1",
    "jquery": "^3.3.1",
    "js-yaml": "^3.7.0",
    "json5": "^0.5.1",
    "jsplumb": "2.3.5",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "lodash": "^4.13.1",
    "log4javascript": "^1.4.15",
    "moment": "^2.22.0",
    "monaco-editor": "^0.10.1",
    "monaco-languageclient": "^0.2.0",
    "mousetrap": "^1.6.0",
    "prop-types": "^15.5.8",
    "rc-menu": "^5.0.10",
    "rc-tabs": "^9.1.10",
    "react": "^16.2.0",
    "react-ace": "^5.8.0",
    "react-autosuggest": "^9.0.1",
    "react-bootstrap": "^0.31.5",
    "react-collapsible": "^2.0.3",
