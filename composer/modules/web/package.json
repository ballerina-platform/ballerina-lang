    "reconnecting-websocket": "^3.2.2",
    "semantic-ui-less": "^2.2.14",
    "semantic-ui-react": "^0.78.3",
    "swagger-editor-dist": "^3.1.18",
    "uuid": "^3.1.0",
    "vscode-ws-jsonrpc": "0.0.1-alpha.5",
    "webpack": "^2.2.1"
  }
}
