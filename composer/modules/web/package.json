{
  "name": "ballerina-composer",
  "version": "0.970.0",
  "description": "A web based tool for creating integrations in ballerina language.",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "build-electron": "cross-env NODE_ENV=electron webpack --config webpack.config.js",
    "dev": "npm run clean && webpack-dev-server --hot --inline --progress",
    "dev-electron": "cross-env NODE_ENV=electron-dev webpack-dev-server --hot --inline --progress",
    "test-spec:keep-building": "cross-env NODE_ENV=test-source-gen-dev webpack --watch",
    "test-spec:run": "mocha --require source-map-support/register ./src/plugins/ballerina/tests/js/spec/setup.js ./target/ballerina-test.js",
    "test-spec:run-ignore-ws": "cross-env IGNORE_WS=true mocha --require source-map-support/register ./src/plugins/ballerina/tests/js/spec/setup.js ./target/ballerina-test.js",
    "test-spec:debug": "cross-env SOURCE_DEBUG=true mocha --require source-map-support/register ./src/plugins/ballerina/tests/js/spec/setup.js ./target/ballerina-test.js",
    "test-spec:inspect": "cross-env mocha --inspect --debug-brk --require source-map-support/register ./src/plugins/ballerina/tests/js/spec/setup.js ./target/ballerina-test.js",
    "test-spec": "cross-env NODE_ENV=test mocha-webpack --require ./src/plugins/ballerina/tests/js/spec/setup.js --webpack-config ./webpack.config.js ./src/plugins/ballerina/tests/js/spec/ballerina-test.js;",
    "test": "cross-env NODE_ENV=test mocha-webpack --recursive --require ./src/plugins/ballerina/tests/js/spec/setup.js --webpack-config ./webpack.config.js 'src/plugins/ballerina/tests/js/spec/**/*test.js'",
    "test-ui": "cross-env NODE_ENV=test mocha-webpack --require ./src/plugins/ballerina/tests/js/spec/setup.js --webpack-config ./webpack.config.js ./react-test/__tests__/component-tests.js;",
    "test-debug": "cross-env NODE_ENV=test node --inspect ./node_modules/mocha-webpack/lib/cli --require ./src/plugins/ballerina/tests/js/spec/setup.js --webpack-config ./webpack.config.js 'src/plugins/ballerina/tests/js/spec/*test.js' --timeout 600000;",
    "lint": "eslint . --ext .jsx --ext .js",
    "lint-diff": "node lint-diff.js js jsx",
    "lintfile": "eslint",
    "gen-source-gen": "cross-env NODE_PATH=src babel-node ./source-gen/gen-source-gen.js",
    "gen-source-gen-java": "cross-env NODE_PATH=src babel-node ./source-gen/gen-source-gen-java.js",
    "gen-default-nodes": "cross-env NODE_PATH=src babel-node ./tools/gen-default-nodes/gen-default-nodes",
    "check": "npm outdated -depth 0"
  },
  "lint-staged": {
    "linters": {
      "*.{js,jsx}": [
        "lintfile --fix --quiet"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ballerinalang/composer.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ballerinalang/composer/issues"
  },
  "homepage": "https://github.com/ballerinalang/composer#readme",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-module-resolver": "^2.7.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "chai": "^3.5.0",
    "chai-jest-snapshot": "^1.3.0",
    "chalk": "^2.3.0",
    "circular-dependency-plugin": "^3.0.0",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.26.2",
    "enzyme": "^3.3.0",
    "enzyme-to-json": "^3.3.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^9.11.0",
    "lint-staged": "^5.0.0",
    "mocha": "^3.2.0",
    "mocha-webpack": "^0.7.0",
    "node-sass": "^4.7.2",
    "npm": "^4.2.0",
    "progress-bar-webpack-plugin": "^1.10.0",
    "react-notification-system": "^0.2.15",
    "react-test-renderer": "^16.2.0",
    "redux": "^3.7.2",
