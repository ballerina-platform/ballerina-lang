import ballerina/grpc;
import ballerina/log;

listener grpc:Listener checkoutep = new (9108);

@grpc:ServiceDescriptor {
    descriptor: ROOT_DESCRIPTOR_17,
    descMap: getDescriptorMap17()
}
service CheckoutService on checkoutep {

    resource function PlaceOrder(grpc:Caller caller, PlaceOrderRequest value) {
        log:printInfo("PlaceOrderRequest: " + value.toString());

        var addr = value.address;
        log:printInfo("Address: " + addr.toString());
        if addr is Address2 {
            PlaceOrderResponse response = {'order: "This is a address"};
            var result = caller->send(response);
            if (result is error) {
                log:printError("Error while sending response.", result);
            }
        } else {
            log:printInfo("value of Address: " + addr.toString());
            PlaceOrderResponse response = {'order: "This is not an address: " + addr.toString()};
            var result = caller->send(response);
            if (result is error) {
                log:printError("Error while sending response.", result);
            }
        }
        var complete = caller->complete();
        if (complete is error) {
            log:printError("Error while completing the response.", complete);
        }
    }
}

public type PlaceOrderRequest record {|
    string user_id = "";
    string user_currency = "";
    Address2? address = ();
    string email = "";
    CreditCardInfo? credit_card = ();

|};


public type PlaceOrderResponse record {|
    string 'order = "";

|};


public type Address2 record {|
    string street_address = "";
    string city = "";
    string state = "";
    string country = "";
    int zip_code = 0;

|};


public type CreditCardInfo record {|
    string credit_card_number = "";
    int credit_card_cvv = 0;
    int credit_card_expiration_year = 0;
    int credit_card_expiration_month = 0;

|};

const string ROOT_DESCRIPTOR_17 = "";
function getDescriptorMap17() returns map<string> {
    return {
        "checkoutservice.proto":""

    };
}

