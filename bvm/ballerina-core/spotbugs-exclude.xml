<FindBugsFilter>
    <Match>
        <Package name="org.ballerinalang.model.values"/>
    </Match>
    <Match>
        <Package name="org.ballerinalang.bre*"/>
    </Match>
    <Match>
        <Package name="org.ballerinalang.model.statements"/>
    </Match>
    <Match>
        <Package name="org.ballerinalang.core.model.expressions"/>
    </Match>
    <Match>
        <Package name="org.ballerinalang.core.model.types"/>
    </Match>
    <Match>
        <Package name="org.ballerinalang.core.model"/>
    </Match>
    <Match>
        <Package name="org.ballerinalang.core.interpreter"/>
    </Match>
    <Match>
        <Package name="org.ballerinalang.core.parser"/>
    </Match>
	<Match>
        <OR>
            <Package name="org.ballerinalang.core.nativeimpl*"/>
        </OR>
        <OR>
            <Bug pattern="EI_EXPOSE_REP"/>
        </OR>
    </Match>
</FindBugsFilter>
