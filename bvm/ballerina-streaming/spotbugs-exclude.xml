<FindBugsFilter>

    <!--annotations-->
    <Match>
        <Package name="~org\.ballerinalang\.siddhi\.annotation\.classindex.*"/>
        <Bug pattern="SIC_INNER_SHOULD_BE_STATIC_ANON"/>
    </Match>

    <!--api-->
    <Match>
        <Class name="org.ballerinalang.siddhi.query.api.expression.AttributeFunction"/>
        <Bug pattern="EI_EXPOSE_REP"/>
    </Match>
    <Match>
        <Class name="org.ballerinalang.siddhi.query.api.execution.query.input.handler.StreamFunction"/>
        <Bug pattern="EI_EXPOSE_REP"/>
    </Match>
    <Match>
        <Class name="org.ballerinalang.siddhi.query.api.expression.AttributeFunction"/>
        <Bug pattern="EI_EXPOSE_REP2"/>
    </Match>
    <Match>
        <Class name="org.ballerinalang.siddhi.query.api.execution.query.input.handler.StreamFunction"/>
        <Bug pattern="EI_EXPOSE_REP2"/>
    </Match>
    <Match>
        <Class name="org.ballerinalang.siddhi.query.api.execution.query.output.stream.UpdateOrInsertStream"/>
        <Bug pattern="SE_BAD_FIELD"/>
    </Match>
    <Match>
        <Class name="org.ballerinalang.siddhi.query.api.execution.query.output.stream.UpdateStream"/>
        <Bug pattern="SE_BAD_FIELD"/>
    </Match>
    <Match>
        <Class name="org.ballerinalang.siddhi.query.api.execution.query.selection.BasicSelector"/>
        <Bug pattern="SE_NO_SERIALVERSIONID"/>
    </Match>
    <Match>
        <Class name="org.ballerinalang.siddhi.query.api.execution.query.StoreQuery"/>
        <Bug pattern="SE_BAD_FIELD"/>
    </Match>
    <Match>
        <Class name="org.ballerinalang.siddhi.query.api.definition.WindowDefinition"/>
        <Bug pattern="UWF_FIELD_NOT_INITIALIZED_IN_CONSTRUCTOR"/>
    </Match>
    <Match>
        <Class name="org.ballerinalang.siddhi.query.api.definition.AbstractDefinition"/>
        <Bug pattern="DM_CONVERT_CASE"/>
    </Match>

    <Match>
        <Package name="~org\.ballerinalang\.siddhi\.query\.api\.execution\.query\.output.*"/>
        <Bug pattern="SE_NO_SERIALVERSIONID"/>
    </Match>

    <Match>
        <Package name="~org\.ballerinalang\.siddhi\.query\.api\.execution\.query\.input.*"/>
        <Bug pattern="SE_NO_SERIALVERSIONID"/>
    </Match>

    <Match>
        <Package name="~org\.ballerinalang\.siddhi\.query\.api.*"/>
        <Bug pattern="EI_EXPOSE_REP, EI_EXPOSE_REP2"/>
    </Match>


    <!--compiler-->
    <Match>
        <Class name="org.ballerinalang.siddhi.query.compiler.internal.SiddhiQLBaseVisitorImpl"/>
        <Bug pattern="EI_EXPOSE_REP, EI_EXPOSE_REP2"/>
    </Match>
    <Match>
        <Class name="org.ballerinalang.siddhi.query.compiler.SiddhiQLLexer"/>
        <Bug pattern="EI_EXPOSE_REP, MS_FINAL_PKGPROTECT, MS_PKGPROTECT, SF_SWITCH_NO_DEFAULT, SF_SWITCH_NO_DEFAULT"/>
    </Match>
    <Match>
        <Class name="org.ballerinalang.siddhi.query.compiler.SiddhiQLParser"/>
        <Bug pattern="MF_CLASS_MASKS_FIELD, BC_UNCONFIRMED_CAST, EI_EXPOSE_REP, MS_PKGPROTECT, SF_SWITCH_NO_DEFAULT"/>
    </Match>
    <Match>
        <Class name="org.ballerinalang.siddhi.query.compiler.SiddhiQLParser$CollectContext"/>
        <Bug pattern="MF_CLASS_MASKS_FIELD"/>
    </Match>
    <Match>
        <Class name="org.ballerinalang.siddhi.query.compiler.exception.SiddhiParserException"/>
        <Bug pattern="EI_EXPOSE_REP, EI_EXPOSE_REP2"/>
    </Match>

    <!--core-->
    <Match>
        <Class name="org.ballerinalang.siddhi.core.query.output.ratelimit.event.AllPerEventOutputRateLimiter"/>
        <Bug pattern="VO_VOLATILE_INCREMENT"/>
