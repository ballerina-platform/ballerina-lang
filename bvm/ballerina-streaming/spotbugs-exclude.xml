    </Match>
    <Match>
        <Class name="org.ballerinalang.siddhi.core.executor.condition.OrConditionExpressionExecutor"/>
        <Bug pattern="RC_REF_COMPARISON_BAD_PRACTICE_BOOLEAN"/>
    </Match>
    <Match>
        <Class name="org.ballerinalang.siddhi.core.util.parser.helper.DefinitionParserHelper$2"/>
        <Bug pattern="SE_BAD_FIELD"/>
    </Match>
    <Match>
        <Class name="org.ballerinalang.siddhi.core.util.SiddhiAppRuntimeBuilder"/>
        <Bug pattern="RV_RETURN_VALUE_OF_PUTIFABSENT_IGNORED"/>
    </Match>
    <Match>
        <class name="org.ballerinalang.siddhi.core.query.processor.stream.AbstractStreamProcessor"/>
        <Bug pattern="IS2_INCONSISTENT_SYNC"/>
    </Match>
    <Match>
        <class name="org.ballerinalang.siddhi.core.query.selector.QuerySelector"/>
        <Bug pattern="IS2_INCONSISTENT_SYNC"/>
    </Match>
    <Match>
        <class name="org.ballerinalang.siddhi.core.query.selector.attribute.aggregator.AttributeAggregator"/>
        <Bug pattern="IS2_INCONSISTENT_SYNC"/>
    </Match>
    <Match>
        <class name="org.ballerinalang.siddhi.core.stream.StreamJunction"/>
        <Bug pattern="IS2_INCONSISTENT_SYNC"/>
    </Match>
    <Match>
        <class name="org.ballerinalang.siddhi.core.util.timestamp.EventTimeBasedMillisTimestampGenerator"/>
        <Bug pattern="IS2_INCONSISTENT_SYNC"/>
    </Match>
    <Match>
        <class name="org.ballerinalang.siddhi.core.event.stream.StreamEvent"/>
        <Bug pattern="UC_USELESS_OBJECT"/>
    </Match>
    <Match>
        <Class name="org.ballerinalang.siddhi.core.executor.VariableExpressionExecutor"/>
        <Bug pattern="EI_EXPOSE_REP"/>
    </Match>
    <Match>
        <Class name="org.ballerinalang.siddhi.core.stream.output.sink.distributed.DistributedTransport"/>
        <Bug pattern="EI_EXPOSE_REP"/>
    </Match>
    <Match>
        <Class name="org.ballerinalang.siddhi.core.query.selector.attribute.processor.executor.AbstractAggregationAttributeExecutor"/>
        <Bug pattern="URF_UNREAD_PUBLIC_OR_PROTECTED_FIELD"/>
    </Match>
    <Match>
        <Class name="org.ballerinalang.siddhi.core.util.transport.InMemoryBroker$Subscriber"/>
        <Bug pattern="DE_MIGHT_IGNORE"/>
    </Match>
    <Match>
        <Class name="org.ballerinalang.siddhi.core.util.transport.InMemoryBroker$MessageBroker"/>
        <Bug pattern="SIC_INNER_SHOULD_BE_STATIC_ANON , DE_MIGHT_IGNORE"/>
    </Match>
    <Match>
        <Class name="org.ballerinalang.siddhi.core.util.parser.JoinInputStreamParser"/>
        <Bug pattern="SF_SWITCH_NO_DEFAULT"/>
    </Match>
    <Match>
        <Class name="org.ballerinalang.siddhi.core.query.selector.attribute.aggregator.incremental.AvgIncrementalAttributeAggregator"/>
        <Bug pattern="EI_EXPOSE_REP"/>
    </Match>
    <Match>
        <Class name="org.ballerinalang.siddhi.core.query.selector.attribute.aggregator.incremental.CountIncrementalAttributeAggregator"/>
        <Bug pattern="EI_EXPOSE_REP"/>
    </Match>
    <Match>
        <Class name="org.ballerinalang.siddhi.core.query.selector.attribute.aggregator.incremental.SumIncrementalAttributeAggregator"/>
        <Bug pattern="EI_EXPOSE_REP"/>
    </Match>
    <Match>
        <Class name="org.ballerinalang.siddhi.core.exception.SiddhiAppCreationException"/>
        <Bug pattern="EI_EXPOSE_REP"/>
    </Match>
    <Match>
        <Class name="org.ballerinalang.siddhi.core.exception.StoreQueryCreationException"/>
        <Bug pattern="EI_EXPOSE_REP"/>
    </Match>
    <Match>
        <Class name="org.ballerinalang.siddhi.core.table.holder.IndexEventHolder"/>
        <Bug pattern="EI_EXPOSE_REP"/>
    </Match>

    <Match>
        <Package name="~org\.ballerinalang\.siddhi\.core\.query\.output\.ratelimit\.time.*"/>
        <Bug pattern="IS2_INCONSISTENT_SYNC"/>
    </Match>
    <Match>
        <Package name="~org\.ballerinalang\.siddhi\.core.*"/>
        <Bug pattern="EI_EXPOSE_REP2, SIC_INNER_SHOULD_BE_STATIC"/>
    </Match>
    <Match>
        <Package name="~org\.ballerinalang\.siddhi\.core\.event.*"/>
        <Bug pattern="EI_EXPOSE_REP"/>
    </Match>
    <Match>
        <Package name="~org\.ballerinalang\.siddhi\.core\.query\.selector\.attribute\.aggregator\.incremental.*"/>
        <Bug pattern="EI_EXPOSE_REP"/>
    </Match>

    <Match>
        <Package name="~org\.ballerinalang\.siddhi\.core.*"/>
    </Match>

    <!--other-->
    <Match>
        <Package name="~org\.ballerinalang\.siddhi\.performance.*"/>
    </Match>
    <Match>
        <Package name="~org\.ballerinalang\.siddhi\.sample.*"/>
    </Match>

</FindBugsFilter>